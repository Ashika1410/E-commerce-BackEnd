{
    "version": 2,
    "build": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        },
        {
            "$schema": "https://openapi.vercel.sh/vercel.json"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.js"
        }
    ]
}